    "dae": {
      "macro": {
        "name": "Acid Arrow v4",
        "img": "icons/magic/acid/projectile-bolts-salvo-green.webp",
        "type": "script",
        "scope": "global",
        "command": "const scaling = workflow.chatCard.getFlag(\"dnd5e\", \"scaling\") ?? 0;\nconst theItem = rolledItem.clone({\"flags.dnd5e.scaling\": scaling, name: `${rolledItem.name} missed damage`}, {keepId: true});\nconst damageActivity = theItem.system.activities.contents.find(a => a.type === \"damage\");\nif (!damageActivity) {\n    console.error(`${rolledItem.name} could not find damage activity`)\n    return;\n}\nif (workflow.hitTargets.size === 0 && workflow.activity.id !== damageActivity.id) {\n    MidiQOL.completeActivityUse(damageActivity, {consume: false}, {configure: false}, {})\n}",
        "author": "Zl1BOUaPLwDEujaL",
        "ownership": {
          "default": 3
        },
